{
	"name": "mutable-jsx",
	"description": "Reactive library focused on mutability. DOM Expressions is the Render Runtime to commit changes to the DOM way faster than a Virtual DOM",
	"version": "0.1.3",
	"author": "Tito Bouzout",
	"repository": {
		"type": "git",
		"url": "https://github.com/titoBouzout/mutable-jsx"
	},
	"module": "dist/index.js",
	"main": "lib/index.js",
	"types": "types/index.d.ts",
	"scripts": {
		"build": "rollup -c && ncp node_modules/dom-expressions/src/client.d.ts ./types/client.d.ts && ncp node_modules/dom-expressions/src/jsx.d.ts ./types/jsx.d.ts && tsc"
	},
	"dependencies": {
		"@babel/core": "7.19.3",
		"@babel/preset-typescript": "7.18.6",
		"@rollup/plugin-babel": "5.3.1",
		"@rollup/plugin-node-resolve": "14.1.0",
		"@rollup/plugin-replace": "^5.0.2",
		"csstype": "^3.1.0",
		"dom-expressions": "0.34.12",
		"mobx": "^6.6.2",
		"ncp": "2.0.0",
		"rollup": "^2.41.4",
		"typescript": "4.8.4"
	},
	"prettier": {
		"printWidth": 70,
		"useTabs": true,
		"semi": false,
		"singleQuote": true,
		"jsxSingleQuote": false,
		"trailingComma": "all",
		"bracketSpacing": true,
		"jsxBracketSameLine": false,
		"arrowParens": "avoid",
		"quoteProps": "preserve",
		"proseWrap": "never"
	}
}
