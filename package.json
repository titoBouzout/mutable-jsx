{
	"name": "mutable-jsx",
	"description": "Reactive library focused on mutability. DOM Expressions is the Render Runtime to commit changes to the DOM way faster than a Virtual DOM",
	"version": "0.1.4",
	"author": "Tito Bouzout",
	"repository": {
		"type": "git",
		"url": "https://github.com/titoBouzout/mutable-jsx"
	},
	"module": "dist/index.js",
	"main": "dist/index.js",
	"types": "types/index.d.ts",
	"type": "module",
	"scripts": {
		"build": "rollup -c && ncp node_modules/dom-expressions/src/client.d.ts ./types/client.d.ts && ncp node_modules/dom-expressions/src/jsx.d.ts ./types/jsx.d.ts && tsc",
		"test": "rollup -c --watch"
	},
	"dependencies": {
		"@babel/core": "7.21.0",
		"@babel/preset-typescript": "7.21.0",
		"@rollup/plugin-babel": "6.0.3",
		"@rollup/plugin-node-resolve": "15.0.1",
		"@rollup/plugin-replace": "^5.0.2",
		"babel-plugin-jsx-dom-expressions": "^0.35.16",
		"csstype": "^3.1.1",
		"dom-expressions": "^0.35.16",
		"ncp": "2.0.0",
		"rollup": "^3.17.2",
		"typescript": "4.9.5"
	},
	"prettier": {
		"printWidth": 70,
		"useTabs": true,
		"semi": false,
		"singleQuote": true,
		"jsxSingleQuote": false,
		"trailingComma": "all",
		"bracketSpacing": true,
		"jsxBracketSameLine": false,
		"arrowParens": "avoid",
		"quoteProps": "preserve",
		"proseWrap": "never"
	}
}
